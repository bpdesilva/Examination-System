<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - OEMS</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" type="text/css"
          href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="~/Content/MKT/css/material-kit.css?v=2.1.0" />
    <!--   Core JS Files   -->
    <script src="~/Content/MKT/js/core/jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/MKT/js/core/popper.min.js" type="text/javascript"></script>
    <script src="~/Content/MKT/js/core/bootstrap-material-design.min.js" type="text/javascript"></script>
    <script src="~/Content/MKT//js/plugins/moment.min.js"></script>

    <script src="~/Content/MKT/js/plugins/nouislider.min.js"></script>
    <script src="~/Content/MKT/js/material-kit.js?v=2.1.0" type="text/javascript"></script>
    <script src="~/Content/MKT/js/user/home.js" type="text/javascript"></script>
    <link rel="stylesheet" href="https://cdn.metroui.org.ua/v4/css/metro-all.min.css">
    <script href="https://cdn.metroui.org.ua/v4/js/metro.min.js"></script>
</head>
<body>
    <div class="container body-content">
        @RenderBody()
        <hr />
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
